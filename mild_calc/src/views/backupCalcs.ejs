            <thead>
                <tr>
                    <th>Row</th>
                    <th>Value</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Unlimited Values</td>
                    <td><%= Math.floor(unlimitedValues * alphaValue1) %></td>
