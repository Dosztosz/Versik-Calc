                    <td><%= Math.floor(unlimitedValuesV2 * alphaValue3 * alphaValue3) %></td>
                </tr>
                <tr>
                    <td>Alpha=</td>
                    <td><%= alphaValue1 %></td>
                    <td><%= alphaValue2 %></td>
                    <td><%= alphaValue3 %></td>
                    <td><%= alphaValue1 %></td>
                    <td><%= alphaValue2 %></td>
                    <td><%= alphaValue3 %></td>
                </tr>
            </tbody>
        </table>